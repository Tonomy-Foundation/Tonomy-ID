{
    "name": "tonomy-id",
    "version": "0.1.0",
    "scripts": {
        "build:prepare": "ts-node prepare.ts",
        "build:ios:create": "npm run build:prepare && eas device:create",
        "build:ios": "npm run build:prepare && eas build --clear-cache --profile development --platform ios",
        "build:android": "npm run build:prepare && eas build --clear-cache --profile development --platform android",
        "start": "npm run build:prepare && expo start --dev-client --clear",
        "lint": "eslint src test --ext '[.ts, .tsx]' --resolve-plugins-relative-to .",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest",
        "testDebug": "jest -o --watch --coverage=false"
    },
    "dependencies": {
        "@greymass/eosio": "^0.6.4",
        "@react-native-async-storage/async-storage": "~1.17.3",
        "@react-navigation/drawer": "^6.5.0",
        "@react-navigation/native": "^6.0.12",
        "@react-navigation/native-stack": "^6.8.0",
        "deepmerge": "^4.2.2",
        "expo": "~45.0.0",
        "expo-dev-client": "~1.0.1",
        "expo-secure-store": "~11.2.0",
        "expo-status-bar": "~1.3.0",
        "fast-text-encoding": "^1.0.6",
        "jest": "^26.6.3",
        "jest-expo": "^46.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native": "0.68.2",
        "react-native-argon2": "^2.0.1",
        "react-native-exception-handler": "^2.10.10",
        "react-native-gesture-handler": "~2.2.1",
        "react-native-paper": "^4.12.5",
        "react-native-reanimated": "~2.8.0",
        "react-native-safe-area-context": "4.2.4",
        "react-native-screens": "~3.11.1",
        "react-native-svg": "12.3.0",
        "react-native-web": "0.17.7",
        "zustand": "^4.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@types/react": "~17.0.21",
        "@types/react-native": "~0.66.13",
        "@types/react-native-base64": "^0.2.0",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "argon2": "^0.29.1",
        "babel-plugin-transform-inline-environment-variables": "^0.4.4",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "prettier": "^2.7.1",
        "react-test-renderer": "^17.0.2",
        "ts-node": "^10.9.1",
        "typescript": "~4.3.5"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)|argon2"
        ]
    },
    "private": true,
    "resolutions": {
        "@greymass/eosio": "^0.6.4"
    }
}
