{
    "name": "tonomy-id",
    "version": "0.29.0",
    "scripts": {
        "build:prepare": "tsc --project tsconfig.settings.json && cp -r ./src/config ./build",
        "build:ios:create": "npm run build:prepare && npx eas device:create",
        "build:ios": "npm run build:prepare && npx eas build --clear-cache --profile development --platform ios --no-wait",
        "build:android": "npm run build:prepare && npx eas build --clear-cache --profile development --platform android --no-wait",
        "deploy": "npm run build:prepare && npx eas build --platform all --clear-cache --auto-submit-with-profile=${NODE_ENV} --no-wait --non-interactive",
        "start": "npm run build:prepare && npx expo start --dev-client --clear",
        "lint": "eslint src test --ext '[.ts, .tsx]' --resolve-plugins-relative-to .",
        "lint:fix": "npm run lint -- --fix",
        "test": "jest",
        "testDebug": "jest -o --watch --coverage=false",
        "typeCheck": "tsc",
        "development": "node sdk-package-install.mjs",
        "prepare": "npm run development"
    },
    "dependencies": {
        "@greymass/eosio": "^0.7.0",
        "@react-native-async-storage/async-storage": "~1.17.3",
        "@react-navigation/bottom-tabs": "^6.5.0",
        "@react-navigation/drawer": "^6.5.0",
        "@react-navigation/native": "^6.0.12",
        "@react-navigation/native-stack": "^6.8.0",
        "@tonomy/tonomy-id-sdk": "^0.5.0",
        "@tonomy/tonomy-id-test": "^1.0.11",
        "base-64": "^1.0.0",
        "big-integer": "^1.6.51",
        "deepmerge": "^4.2.2",
        "expo": "^47.0.0",
        "expo-barcode-scanner": "~11.3.0",
        "expo-camera": "~13.1.0",
        "expo-dev-client": "~2.0.1",
        "expo-linking": "~3.2.3",
        "expo-local-authentication": "~13.0.2",
        "expo-notifications": "~0.17.0",
        "expo-secure-store": "~12.0.0",
        "expo-status-bar": "~1.4.2",
        "expo-web-browser": "^12.0.0",
        "fast-text-encoding": "^1.0.6",
        "react": "18.1.0",
        "react-dom": "18.1.0",
        "react-native": "0.70.5",
        "react-native-argon2": "^2.0.1",
        "react-native-exception-handler": "^2.10.10",
        "react-native-gesture-handler": "~2.8.0",
        "react-native-paper": "^4.12.5",
        "react-native-reanimated": "~2.12.0",
        "react-native-safe-area-context": "4.4.1",
        "react-native-screens": "~3.18.0",
        "react-native-svg": "13.4.0",
        "react-native-web": "~0.18.7",
        "zustand": "^3.7.2"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@types/jest": "^29.5.0",
        "@types/react": "~18.0.24",
        "@types/react-native": "~0.70.6",
        "@types/react-native-base64": "^0.2.0",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "argon2": "^0.29.1",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "babel-plugin-transform-inline-environment-variables": "^0.4.4",
        "eas-cli": "^3.1.1",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "expo-cli": "^5.5.1",
        "jest": "^29.5.0",
        "jest-expo": "^48.0.2",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.6.3"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)|argon2"
        ]
    },
    "private": true,
    "resolutions": {
        "@greymass/eosio": "0.7.0"
    }
}
