{
    "name": "tonomy-id",
    "version": "0.61.0",
    "scripts": {
        "build:prepare": "ts-node prepare.app.config.ts",
        "build:ios:create": "yarn run build:prepare && npx eas device:create",
        "build:ios": "yarn run build:prepare && npx eas build --clear-cache --profile development --platform ios --no-wait",
        "build:android": "yarn run build:prepare && npx eas build --clear-cache --profile development --platform android --no-wait",
        "deploy:demo:android": "EXPO_NODE_ENV=demo EXPO_PLATFORM=android yarn run build:prepare && npx eas build --clear-cache --auto-submit-with-profile=production --no-wait --non-interactive --platform android --profile demo",
        "deploy:demo:ios": "EXPO_NODE_ENV=demo EXPO_PLATFORM=ios yarn run build:prepare && npx eas build --clear-cache --auto-submit-with-profile=production --no-wait --non-interactive --platform ios --profile demo",
        "deploy:demo": "yarn run deploy:demo:android && yarn run deploy:demo:ios",
        "deploy:staging:android": "EXPO_NODE_ENV=staging EXPO_PLATFORM=android yarn run build:prepare && npx eas build --clear-cache --auto-submit-with-profile=production --no-wait --non-interactive --platform android --profile staging",
        "deploy:staging:ios": "EXPO_NODE_ENV=staging EXPO_PLATFORM=ios yarn run build:prepare && npx eas build --clear-cache --auto-submit-with-profile=production --no-wait --non-interactive --platform ios --profile staging",
        "deploy:staging": "yarn run deploy:staging:android && yarn run deploy:staging:ios",
        "start": "NODE_ENV=development yarn run build:prepare && NODE_ENV=development npx expo start --dev-client --clear",
        "lint": "eslint src test --ext '[.ts, .tsx]' --resolve-plugins-relative-to .",
        "lint:fix": "yarn run lint -- --fix",
        "test": "jest",
        "testDebug": "jest -o --watch --coverage=false",
        "typeCheck": "tsc",
        "updateSdkVersion": "./update_sdk_version.sh"
    },
    "dependencies": {
        "@hcaptcha/react-native-hcaptcha": "^1.5.1",
        "@react-native-async-storage/async-storage": "1.18.2",
        "@react-navigation/bottom-tabs": "^6.5.0",
        "@react-navigation/drawer": "^6.5.0",
        "@react-navigation/native": "^6.0.12",
        "@react-navigation/native-stack": "^6.8.0",
        "@tonomy/tonomy-id-sdk": "0.16.0-development.1",
        "@wharfkit/antelope": "^0.7.1",
        "base-64": "^1.0.0",
        "big-integer": "^1.6.51",
        "deepmerge": "^4.2.2",
        "expo": "~49.0.7",
        "expo-barcode-scanner": "~12.5.3",
        "expo-build-properties": "~0.8.3",
        "expo-camera": "~13.4.2",
        "expo-dev-client": "~2.4.8",
        "expo-linking": "~5.0.2",
        "expo-local-authentication": "~13.4.1",
        "expo-notifications": "~0.20.1",
        "expo-secure-store": "~12.3.1",
        "expo-status-bar": "~1.6.0",
        "expo-web-browser": "^12.0.0",
        "fast-text-encoding": "^1.0.6",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "0.72.4",
        "react-native-argon2": "^2.0.1",
        "react-native-autocomplete-input": "^5.3.2",
        "react-native-exception-handler": "^2.10.10",
        "react-native-gesture-handler": "~2.12.0",
        "react-native-modal": "^13.0.1",
        "react-native-paper": "^4.12.5",
        "react-native-reanimated": "~3.3.0",
        "react-native-safe-area-context": "4.6.3",
        "react-native-screens": "~3.22.0",
        "react-native-svg": "13.9.0",
        "react-native-web": "~0.19.6",
        "react-native-webview": "13.2.2",
        "zustand": "^3.7.2"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/jest": "^29.5.0",
        "@types/react": "~18.2.14",
        "@types/react-native-base64": "^0.2.0",
        "@types/react-test-renderer": "^18.0.0",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "argon2": "^0.30.3",
        "babel-plugin-transform-builtin-extend": "^1.1.2",
        "babel-plugin-transform-inline-environment-variables": "^0.4.4",
        "eas-cli": "^4.1.2",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.31.10",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-native": "^4.0.0",
        "jest": "^29.5.0",
        "jest-expo": "^49.0.0",
        "prettier": "^2.7.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    },
    "jest": {
        "preset": "jest-expo",
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)|argon2"
        ]
    },
    "private": true,
    "packageManager": "yarn@3.1.1",
    "resolutions": {
        "@wharfkit/antelope": "0.7.1"
    }
}
