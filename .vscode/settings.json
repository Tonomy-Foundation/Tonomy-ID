{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
    },
    "jest.autoRun": {
        "onSave": false
    },
    "jest.debugMode": false,
    "jest.jestCommandLine": "yarn run test",
    "jest.testExplorer": {
        "enabled": true
    },
}