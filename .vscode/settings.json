{
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "typescript",
        "typescriptreact"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "jest.autoRun": {
        "onSave": false
    },
    "jest.debugMode": false,
    "jest.jestCommandLine": "yarn run test",
    "jest.testExplorer": {
        "enabled": true
    },
    "git.branchPrefix": "feature/",
    "git.branchProtection": [
        "master",
        "main",
        "testnet",
        "development"
    ],
    "conventionalCommits.scopes": [
        "fixed yarn lock"
    ],
}