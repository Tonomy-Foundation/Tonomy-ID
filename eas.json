{
    "cli": {
        "version": "^16.3.3",
        "appVersionSource": "remote"
    },
    "build": {
        "production": {
            "env": {
                "EXPO_NODE_ENV": "production",
                "INFURA_KEY": "$INFURA_KEY",
                "ETHERSCAN_API_KEY": "$ETHERSCAN_API_KEY",
                "SENTRY_PUBLIC_KEY": "$SENTRY_PUBLIC_KEY",
                "SENTRY_PROJECT_ID": "$SENTRY_PROJECT_ID",
                "HCAPTCHA_SITE_KEY": "$HCAPTCHA_SITE_KEY",
                "WALLETCONNECT_PROJECT_ID": "$WALLETCONNECT_PROJECT_ID",
                "VERIFF_API_KEY": "$VERIFF_API_KEY"
            },
            "autoIncrement": true,
            "ios":{                
                "image": "macos-sonoma-14.6-xcode-16.0" 
            },
            "node": "22.3.0"
        },
        "testnet": {
            "env": {
                "EXPO_NODE_ENV": "testnet",
                "DEBUG": "tonomy*"
            },
            "autoIncrement": true,
            "ios":{                
                "image": "macos-sonoma-14.6-xcode-16.0" 
            },
            "node": "22.3.0"
        },
        "testnet-internal": {
            "env": {
                "EXPO_NODE_ENV": "testnet",
                "DEBUG": "tonomy*"
            },
            "distribution": "internal",
            "node": "22.3.0"
        },
        "staging": {
            "env": {
                "EXPO_NODE_ENV": "staging",
                "DEBUG": "tonomy*",
                "INFURA_KEY": "$INFURA_KEY",
                "ETHERSCAN_API_KEY": "$ETHERSCAN_API_KEY",
                "SENTRY_PUBLIC_KEY": "$SENTRY_PUBLIC_KEY",
                "SENTRY_PROJECT_ID": "$SENTRY_PROJECT_ID",
                "HCAPTCHA_SITE_KEY": "$HCAPTCHA_SITE_KEY",
                "WALLETCONNECT_PROJECT_ID": "$WALLETCONNECT_PROJECT_ID",
                "VERIFF_API_KEY": "$VERIFF_API_KEY"
            },
            "ios":{                
                "image": "macos-sonoma-14.6-xcode-16.0" 
            },
            "autoIncrement": true,
            "node": "22.3.0"
        },
        "staging-internal": {
            "env": {
                "EXPO_NODE_ENV": "staging",
                "DEBUG": "tonomy*",
                "INFURA_KEY": "$INFURA_KEY",
                "ETHERSCAN_API_KEY": "$ETHERSCAN_API_KEY",
                "SENTRY_PUBLIC_KEY": "$SENTRY_PUBLIC_KEY",
                "SENTRY_PROJECT_ID": "$SENTRY_PROJECT_ID",
                "HCAPTCHA_SITE_KEY": "$HCAPTCHA_SITE_KEY",
                "WALLETCONNECT_PROJECT_ID": "$WALLETCONNECT_PROJECT_ID",
                "VERIFF_API_KEY": "$VERIFF_API_KEY"
            },
            "distribution": "internal",
            "node": "22.3.0"
        },
        "development": {
            "env": {
                "EXPO_NODE_ENV": "development",
                "DEBUG": "tonomy*"
            },
            "developmentClient": true,
            "distribution": "internal",
            "node": "22.3.0"
        }
    },
    "submit": {
        "production": {
            "android": {
                "serviceAccountKeyPath": "./google-service-key.json",
                "track": "production"
            },
            "ios": {
                "ascAppId": "1663471436",
                "appleTeamId": "6BLD42QR78",
                "ascApiKeyId": "83XQ25UWSK",
                "ascApiKeyPath": "./apple-asc-api-key.p8",
                "ascApiKeyIssuerId": "ba87a564-cdb7-4a94-9e58-dbcb7d2e8309",
            }
        },
        "appleTestnet": {
            "ios": {
                "ascAppId": "6476114575",
                "appleTeamId": "6BLD42QR78",
                "ascApiKeyId": "83XQ25UWSK",
                "ascApiKeyPath": "./apple-asc-api-key.p8",
                "ascApiKeyIssuerId": "ba87a564-cdb7-4a94-9e58-dbcb7d2e8309"
            }
        },
        "appleProduction": {
            "ios": {
                "ascAppId": "6482296993",
                "appleTeamId": "6BLD42QR78",
                "ascApiKeyId": "83XQ25UWSK",
                "ascApiKeyPath": "./apple-asc-api-key.p8",
                "ascApiKeyIssuerId": "ba87a564-cdb7-4a94-9e58-dbcb7d2e8309"
            }
        }
    }
}