{
    "cli": {
        "version": ">= 2.1.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "staging": {
            "autoIncrement": true
        },
        "demo": {
            "env": {
                "MY_EXPO_VAR": "demo",
                "OTHER_VAR": "value"
            },
            "autoIncrement": true
        },
        "production": {
            "env": {
                "OTHER_VAR": "value2"
            },
            "autoIncrement": true
        }
    },
    "submit": {
        "staging": {
            "android": {
                "serviceAccountKeyPath": "./google-service-key.json",
                "track": "production"
            },
            "ios": {
                "ascAppId": "1663471436",
                "appleTeamId": "6BLD42QR78",
                "ascApiKeyId": "8VMB5377W8",
                "ascApiKeyPath": "./apple-asc-api-key.p8",
                "ascApiKeyIssuerId": "ba87a564-cdb7-4a94-9e58-dbcb7d2e8309"
            }
        },
        "demo": {
            "android": {
                "serviceAccountKeyPath": "./google-service-key.json",
                "track": "production"
            }
        }
    }
}