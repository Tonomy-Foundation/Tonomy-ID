{
    "cli": {
        "version": ">= 2.1.0",
        "appVersionSource": "remote"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal"
        },
        "production": {}
    },
    "submit": {
        "staging": {
            "autoIncrement": true,
            "android": {
                "serviceAccountKeyPath": "./google-service-key.json",
                "track": "production"    
            }
        },
        "production": {
            "autoIncrement": true
        }
    }
}
